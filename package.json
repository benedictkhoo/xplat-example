{
  "name": "xplat-example",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start.electron.client": "npm run prepare.electron.client && npm-run-all -p serve.electron.client.target serve.electron.client",
    "start.nativescript.client.ios": "cd apps/nativescript-client && tns run ios --emulator --bundle --hmr",
    "start.nativescript.client.android": "cd apps/nativescript-client && tns run android --emulator --bundle --hmr",
    "start.web.client": "npm run postinstall.web && ng serve web-client --aot",
    "build.electron.client": "npm run prepare.electron.client && ng build electron-client --prod --base-href ./",
    "build.electron.client.local": "npm run build.electron.client && electron dist/apps/electron-client",
    "build.electron.client.linux": "npm run build.electron.client && cd dist/apps/electron-client && npx electron-builder build --linux",
    "build.electron.client.windows": "npm run build.electron.client && cd dist/apps/electron-client && npx electron-builder build --windows",
    "build.electron.client.mac": "npm run build.electron.client && cd dist/apps/electron-client && npx electron-builder build --mac",
    "build.nativescript.client.ios": "cd apps/nativescript-client && npm run build.ios",
    "build.nativescript.client.android": "cd apps/nativescript-client && npm run build.android",
    "build.web.client": "npm run postinstall.web && ng build web-client",
    "test.web.client": "npm run postinstall.web && ng test web-client",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e.web.client": "npm run postinstall.web && ng e2e web-client-e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "clean.nativescript.client": "cd apps/nativescript-client && npx rimraf -- hooks node_modules platforms package-lock.json && npm i && npx rimraf -- package-lock.json",
    "postinstall": "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true npx electron-builder install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "prepare.electron.client": "npm run postinstall.electron && tsc -p apps/electron-client/tsconfig.json",
    "serve.electron.client.target": "ng serve electron-client --aot",
    "serve.electron.client": "wait-on http-get://localhost:4200/ && electron apps/electron-client/src --serve",
    "release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "core-js": "^2.5.7",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.26",
    "@nrwl/nx": "7.1.1",
    "@ngrx/effects": "6.1.2",
    "@ngrx/router-store": "6.1.2",
    "@ngrx/store": "6.1.2",
    "@ngx-translate/core": "~11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "@xe/scss": "file:libs/scss",
    "reflect-metadata": "^0.1.12",
    "nativescript-angular": "~7.0.2",
    "nativescript-ngx-fonticon": "^4.2.0",
    "nativescript-theme-core": "^1.0.4",
    "tns-core-modules": "~5.0.5",
    "@xe/web": "file:xplat/web"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "7.1.2",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@ngrx/store-devtools": "6.1.2",
    "@nrwl/schematics": "7.1.1",
    "@nstudio/schematics": "^7.0.1",
    "@semantic-release/changelog": "3.0.1",
    "@semantic-release/git": "7.0.5",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "3.0.10",
    "electron-builder": "20.38.2",
    "electron-installer-dmg": "2.0.0",
    "electron-packager": "12.2.0",
    "electron-reload": "1.3.0",
    "electron-store": "2.0.0",
    "electron-updater": "4.0.5",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "0.4.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ngrx-store-freeze": "0.2.4",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "protractor": "~5.4.1",
    "semantic-release": "15.12.4",
    "tns-platform-declarations": "~5.0.5",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "wait-on": "2.1.0"
  },
  "xplat": {
    "prefix": "xe"
  }
}
